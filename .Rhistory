# Function for creating leaflet map
map_func <- function(data, title, title2) {
data %>%
leaflet(width = "100%") %>%
addProviderTiles(provider = "CartoDB.Positron") %>%
addPolygons(
label = ~ str_extract(NAME.y, "^([^,]*)"),
stroke = FALSE,
smoothFactor = 0,
fillOpacity = 0.7,
color = ~ pal(estimate)
) %>%
addLegend("bottomright",
pal = pal,
values = ~estimate,
title = title,
opacity = 1
) %>%
addControl(title2, position = "topright")
}
